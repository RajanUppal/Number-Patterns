/*
C++ program to print the following number pattern.
Input : 5
Output :
54321
54321
54321
54321
54321
*/

#include <iostream>
using namespace std;

int main()
{
	int n,i,j;
	cout<<"Enter number of rows : ";
	cin>>n;
	i=1;
	while (i<=n)
	{
		j=1;
		while (j<=n)
		{
			cout<< n - j + 1;
			j++;
		}
		cout<<endl;
		i++;
	}
}
