/*
C++ program to print the following number pattern.
Input : 5
Output :
11111
22222
33333
44444
55555
*/

#include <iostream>
using namespace std;

int main()
{
	int n,i,j;
	cout<<"Enter number of rows : ";
	cin>>n;
	i=1;
	while (i<=n)
	{
		j=1;
		while (j<=n)
		{
			cout<<i;
			j++;
		}
		cout<<endl;
		i++;
	}
}
